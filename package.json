    "node-fetch": "^2.6.7",
    "npm": "^8.8.0",
    "prettier": "2.6.2",
    "ts-node": "^10.7.0",
    "typescript": "^4.4.4",
    "vite": "^2.9.18",
    "vite-plugin-solid": "^2.3.0"
  },
  "dependencies": {
    "@arrows/array": "^1.4.1",
    "canvas-txt": "^3.0.0",
    "chess.js": "^0.13.2",
    "common-tags": "^1.8.2",
    "gif.js": "^0.2.0",
    "h264-mp4-encoder": "^1.0.12",
    "hammerjs": "^2.0.8",
    "howler": "^2.2.3",
