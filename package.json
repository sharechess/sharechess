  "scripts": {
    "start": "vite --host",
    "dev": "vite --host",
    "build": "vite build",
    "build:all": "npm run gen && vite build",
    "preview": "vite preview",
    "gen": "npm run gen:templates && npm run gen:styles && npm run gen:zips",
    "gen:templates": "ts-node scripts/createBoardTemplates.ts",
    "gen:styles": "npm run gen:styles:boards && npm run gen:styles:pieces",
    "gen:styles:boards": "ts-node scripts/createBoardsStylesheets.ts",
    "gen:styles:pieces": "ts-node scripts/createPiecesStylesheets.ts",
    "gen:zips": "npm run gen:zips:boards && npm run gen:zips:pieces",
    "gen:zips:boards": "ts-node scripts/createBoardsZIPs.ts",
    "gen:zips:pieces": "ts-node scripts/createPiecesZIPs.ts",
    "gen:puzzles": "ts-node scripts/createDailyPuzzles.ts"
  },
  "devDependencies": {
    "@types/canvas-txt": "^3.0.0",
    "@types/chess.js": "^0.11.2",
    "@types/common-tags": "^1.8.1",
    "@types/gif.js": "^0.2.2",
    "@types/hammerjs": "^2.0.41",
    "@types/howler": "^2.2.6",
    "@types/imagemin": "^8.0.0",
    "@types/mime": "^2.0.3",
    "@types/node": "^16.11.56",
    "@types/node-fetch": "^2.6.2",
    "@types/prettier": "^2.6.1",
    "@types/ua-parser-js": "^0.7.36",
    "@vitejs/plugin-legacy": "^1.8.1",
    "canvas": "^2.11.2",
    "imagemin": "^7.0.1",
